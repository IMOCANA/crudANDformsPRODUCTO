
{% extends 'base.html.twig' %}

{% block body %}

    <div>
        <form method="post" action="{{ action }}" novalidate="novalidate">
            <div>
                {{ dump(form.vars.value) }}
            </div>

            <div>{{ form_errors(form.nombre) }}{{ form_label(form.nombre) }}: {{ form_widget(form.nombre) }}</div>
            <div>{{ form_errors(form.descripcion) }}{{ form_label(form.descripcion) }}: {{ form_widget(form.descripcion) }}</div>
            <div>{{ form_errors(form.precio) }}{{ form_label(form.precio) }}: {{ form_widget(form.precio) }}</div>
            <div>{{ form_errors(form.disponible) }}{{ form_label(form.disponible) }}: {{ form_widget(form.disponible) }}</div>
            {{ form_rest(form) }}
            <input type="submit" value="Insert">
        </form>


    </div>

{% endblock %}























